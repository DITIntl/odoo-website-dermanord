                    </div>
                    <div class="oe_snippet_body oe_share">
                        <div class="col-md-12 mb16 mt16">
                            <div class="col-md-6">
                                <img class="img img-responsive" src="/website/static/src/img/banner/greenfields.jpg" style="max-height: 400px; margin: auto;"/>
                                <p class="text-center">Image text</p>
                            </div>
                            <div class="col-md-6">
                                <img class="img img-responsive" src="/website/static/src/img/banner/greenfields.jpg" style="max-height: 400px; margin: auto;"/>
                                <p class="text-center">Image text</p>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- End of two column image -->
            </xpath>

            <!-- Customize Features -->
            <xpath expr="//div[@id='snippet_feature']" position="inside">

            </xpath>

