        <!-- Snippet options -->
        <template id="more_snippet_options" inherit_id="website.snippet_options" name="More Snippet Options">

            <xpath expr="." position="inside">
                <div data-snippet-option-id="blog_banner_option" data-selector=".blog_banner">
                    <li class="dropdown-submenu">
                        <t t-set="blogs" t-value="request.env['blog.blog'].search([])"/>
                        <t t-if="len(blogs) &gt; 0">
                            <a tabindex="-1" href="#">Blogs</a>
                            <ul class="dropdown-menu">
                                <t t-foreach="blogs" t-as="b">
                                    <li><a t-att-data-value="b.id" class="oe_blog_banner_change"><t t-esc="b.name"/></a></li>
                                </t>
                            </ul>
                        </t>
                    </li>
                </div>
                <div data-snippet-option-id="blog_slide_option" data-selector=".blog_slide">
                    <li class="dropdown-submenu">
                        <li class="">
                            <a tabindex="-1" href="#" class="oe_blog_slide">Sale / Promotions</a>
                        </li>
                    </li>
                </div>
                <div data-snippet-option-id="categ_p_option" data-selector=".categ_p_section">
                    <li class="">
                        <a tabindex="-1" href="#" class="oe_p_categories">Categories</a>
                    </li>
                    <li class="dropdown-submenu">
                        <a tabindex="-1" href="#">Columns</a>
                        <ul class="dropdown-menu">
                            <li><a data-value="3" class="oe_c_col_change">Col-3</a></li>
                            <li><a data-value="4" class="oe_c_col_change">Col-4</a></li>
                            <li><a data-value="6" class="oe_c_col_change">Col-6</a></li>
                            <li><a data-value="12" class="oe_c_col_change">Col-12</a></li>
                        </ul>
                    </li>
                </div>
                <div data-snippet-option-id="product_highlights_option" data-selector=".product_highlights">
                    <li class="">
                        <a tabindex="-1" href="#" class="oe_product_highlights">Product Highlights</a>
                    </li>
                    <li class="dropdown-submenu">
                        <a tabindex="-1" href="#">Columns</a>
