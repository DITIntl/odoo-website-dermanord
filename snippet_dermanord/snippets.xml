<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <!-- Customize Snippets -->
        <template id="custom_snippets" inherit_id="website.snippets" name="Custom Snippets">
            <!-- Customize Structure -->
            <xpath expr="//div[@id='snippet_structure']" position="inside">
                <div id="blog_banner_slide">
                    <div class="oe_snippet_thumbnail">
                        <img class="oe_snippet_thumbnail_img" src="/website/static/src/img/blocks/block_banner.png"/>
                        <span class="oe_snippet_thumbnail_title">Experiences</span>
                    </div>
                    <section class="oe_snippet_body blog_banner">
                        <div class="carousel slide carousel-fade" data-ride="carousel" id="blog_carousel" style=" overflow: hidden;">
                            <ol class="carousel-indicators blog_banner_indicators"/>
                            <div class="carousel-inner blog_banner_content"/>
                            <a class="left carousel-control" data-slide="prev" href="#blog_carousel">
                                <i class="fa fa-chevron-left"/>
                            </a>
                            <a class="right carousel-control" data-slide="next" href="#blog_carousel">
                                <i class="fa fa-chevron-right"/>
                            </a>
                        </div>
                    </section>
                </div>

                <div id="blog_slide">
                    <div class="oe_snippet_thumbnail">
                        <img class="oe_snippet_thumbnail_img" src="/website/static/src/img/blocks/block_banner.png"/>
                        <span class="oe_snippet_thumbnail_title">Blog Slide</span>
                    </div>
                    <section class="oe_snippet_body blog_slide mb16">
                        <h3 class="text-center mt32 mb32 oe_bs_title dn_uppercase">Blog Slide</h3>
                        <div class="carousel slide" data-ride="carousel" id="blog_slide">
                            <ol class="carousel-indicators blog_slide_indicators"/>
                            <div class="carousel-inner blog_slide_content"/>
                        </div>
                        <img src="/snippet_dermanord/static/src/img/finger.png" class="touch_finger" style="margin: auto; display: block;"/>
                    </section>
                </div>

                <!--FADE SLIDER-->
                <div id="fade_slide">
                    <div class="oe_snippet_thumbnail">
                        <img class="oe_snippet_thumbnail_img" src="/website/static/src/img/blocks/block_banner.png"/>
                        <span class="oe_snippet_thumbnail_title">Banner Fade</span>
                    </div>
                    <section class="oe_snippet_body">
                        <div class="carousel slide carousel-fade" data-ride="carousel" id="fade_carousel" style=" overflow: hidden;">
                            <ol class="carousel-indicators">
                                <li class="active" data-slide-to="0" data-target="fade_carousel"/>
                                <li data-slide-to="1" data-target="fade_carousel"/>
                                <li data-slide-to="2" data-target="fade_carousel"/>
                            </ol>
                            <div class="carousel-inner">
                                <div class="item active" style="">
                                    <img class="img-responsive" src="/website/static/src/img/fields.jpg" style="position: relative;"/>
                                     <div class="container" style="position: absolute; top: 10%; left: 0%;">
                                        <div class="row content">
                                            <div class="col-md-12 col-sm-12 text-center">
                                                <h3 class="mb32">Deepskin organics guarantees the best organic nourishment for the skin, body and hair</h3>
                                                <p>
                                                    <a class="dn_btn dn_primary" href="">DISCOVER MORE</a>
                                                </p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <a class="left carousel-control" data-slide="prev" href="#fade_carousel">
                                <i class="fa fa-chevron-left"/>
                            </a>
                            <a class="right carousel-control" data-slide="next" href="#fade_carousel">
                                <i class="fa fa-chevron-right"/>
                            </a>
                        </div>
                    </section>
                </div>
                <!--END OF FADE SLIDER-->

                <!-- Notes Cubes -->
                <div id="notes_cubes">
                    <div class="oe_snippet_thumbnail">
                        <img class="oe_snippet_thumbnail_img" src="/snippet_dermanord/static/src/img/notes_cubes.png"/>
