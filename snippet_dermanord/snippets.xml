                                <div class="product_div"/>
                            </div>
                        </div>
                    </section>
                </div><!-- End of product block -->

            </xpath>

            <!-- Customize Content -->
            <xpath expr="//div[@id='snippet_content']" position="inside">

                <!-- Two column image -->
                <div id="two_column_image">
                    <div class="oe_snippet_thumbnail">
                        <img class="oe_snippet_thumbnail_img" src="/snippet_dermanord/static/src/img/two_column_image.png"/>
