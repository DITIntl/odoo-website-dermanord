<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_website_config_settings" model="ir.ui.view">
            <field name="name">Website settings Inherited Reseller Dermanord</field>
            <field name="model">website.config.settings</field>
            <field name="inherit_id" ref="website.view_website_config_settings" />
            <field name="arch" type="xml">
                <group string="Social Media" position="after">
                    <group string="Reseller">
                        <label for="find_reseller_top_img"/>
                        <div name="find_reseller_top_img">
                            <div class="oe_inline">
                                <field name="find_reseller_top_img" widget="image"/>
                            </div>
                        </div>
                    </group>
                </group>
            </field>
        </record>
    </data>
</openerp>
